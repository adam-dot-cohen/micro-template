{

  "AllowedHosts": "*",
  "Infrastructure": {
    "Logging": {
      "Common": {
        "Application": "framework-sample",
        "Environment": "Developer",
        "Version": "1.0.0.0",
        "TenantName": "Aliya"
      },
      "Seq": {
        "Enabled": "true",
        "SeqHostUrl": "http://localhost:5341"
      },
      "Elasticsearch": {
        "Enabled": "true",
        "Url": "http://localhost:5341",
        "AutoRegisterTemplate": true,
        "IndexFormat": "Platform-log-{0:yyyy.MM}"
      }
    }
  }
}
